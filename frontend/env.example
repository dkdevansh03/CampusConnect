# Frontend Environment Variables
# Copy this file to .env.local for local development
# Set these variables in Vercel dashboard for production

# Backend API URL (for production, use your Render backend URL)
VITE_API_URL=https://your-backend-app-name.onrender.com

# Frontend URL (for production, use your Vercel frontend URL)
VITE_FRONTEND_URL=https://your-frontend-app-name.vercel.app
